<Page
	x:Class="EOTools.Translation.DestinationUpdateForm"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:Linq="clr-namespace:Newtonsoft.Json.Linq;assembly=Newtonsoft.Json"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="clr-namespace:EOTools.Translation"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	Title="Destination update"
	d:DesignHeight="720"
	d:DesignWidth="1080     "
	mc:Ignorable="d">

	<DockPanel>
		<DockPanel Width="185" LastChildFill="False">
			<Button
				x:Name="buttonUpdate"
				Height="49"
				Margin="10"
				HorizontalAlignment="Center"
				Click="buttonUpdate_Click"
				Content="Update file"
				DockPanel.Dock="Top" />
			<Button
				x:Name="buttonExport"
				Height="49"
				Margin="10"
				HorizontalAlignment="Center"
				Click="buttonExport_Click"
				Content="Save and push to git"
				DockPanel.Dock="Top" />
			<Button
				Height="49"
				Margin="10"
				HorizontalAlignment="Center"
				Click="Button_Click"
				Content="Open raw"
				DockPanel.Dock="Top" />
		</DockPanel>
	</DockPanel>
</Page>
