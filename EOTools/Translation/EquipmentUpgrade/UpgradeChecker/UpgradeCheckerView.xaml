<Window
	x:Class="EOTools.Translation.EquipmentUpgrade.UpgradeChecker.UpgradeCheckerView"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="clr-namespace:EOTools.Translation.EquipmentUpgrade.UpgradeChecker"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:ui="http://schemas.modernwpf.com/2019"
	Title="Upgrade checks"
	Width="800"
	Height="450"
	d:DataContext="{d:DesignInstance local:UpgradeCheckerViewModel}"
	ui:WindowHelper.UseModernWindowStyle="True"
	mc:Ignorable="d">

	<Window.DataContext>
		<local:UpgradeCheckerViewModel />
	</Window.DataContext>

	<StackPanel>
		<DataGrid>
			<DataGrid.Columns>
				<DataGridTextColumn Header="Ship" />
			</DataGrid.Columns>
		</DataGrid>
	</StackPanel>
</Window>
